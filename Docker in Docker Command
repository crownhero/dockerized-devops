sudo docker run -d --name jenkins -p 8080:8080  -p 50000:50000 -v /etc/apache2:/etc/apache2  -v /etc/systemd/system:/etc/systemd/system  -v /var/run/docker.sock:/var/run/docker.sock   -v /usr/bin/docker:/usr/bin/docker   -v jenkins_data:/var/jenkins_home   jenkins_image:latest sh -c "mkdir -p /var/jenkins_home/.ssh && cp /var/jenkins_home/.ssh/waverlite-new-instance.pem /var/jenkins_home/.ssh/"
chmod 666 /var/run/docker.sock











